{
    "publicUrl": "{{ auth_public_url }}",
    "domain": "{{ domain_name }}",
    "db": {
        "backend": "httpdb"
    },
    "httpdb": {
      "url": "{{ authdb_server_url }}"
    },
    "secretKeyFile": "/data/fxa-auth-server/config/secret-key.json",
    "publicKeyFile": "/data/fxa-auth-server/config/public-key.json",
    "customsUrl": "{{ customs_server_url }}",
    "contentServer": {
        "url": "{{ content_public_url }}"
    },
    "templateServer": {
        "url": "{{ content_public_url }}"
    },
    "smtp":{
        "host": "{{ auth_mail_host }}",
        "port": {{ auth_mail_port }},
        "secure": false,
        "sender":"{{ auth_mail_sender }}",
        "templatePath":"/data/fxa-auth-server/templates/email",
        "redirectDomain": "firefox.com",
        "resendBlackoutPeriod": 0
    },
    "listen": {
        "port": {{ auth_private_port }}
    },
    "snsTopicArn": "{{ auth_sns_arn }}",
    "trustedJKUs": [
        "{{ 123done_trusted_jku }}",
        "{{ 321done_trusted_jku }}",
        "https://marketplace-dev.allizom.org/api/v1/account/fxa-preverify-key/"
    ]
}
